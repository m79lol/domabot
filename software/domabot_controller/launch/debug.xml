<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <node
    pkg="domabot_controller"
    exec="domabot_controller_node"
    output="screen"
    namespace="domabot"
    ros_args="--log-level domabot.domabot_controller:=DEBUG --disable-rosout-logs"
    launch-prefix="gdbserver localhost:3000"
  >
    <param from="$(find-pkg-share domabot_controller)/config/default.yaml" />
  </node>
</launch>